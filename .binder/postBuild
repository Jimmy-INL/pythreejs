#!/usr/bin/env bash
set -eux

pushd js
jlpm --ignore-optional
jlpm build:bundles
jlpm build:all
popd

python -m pip install -vv -e . --no-deps --ignore-installed
python -m pip check

# install labextension
pushd js
jupyter labextension develop . --overwrite
popd

# validate
jupyter nbextension list
jupyter labextension list
